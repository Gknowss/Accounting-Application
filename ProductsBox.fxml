<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="360.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UserController">
   <children>
      <AnchorPane layoutX="-83.0" layoutY="-128.0" prefHeight="200.0" prefWidth="200.0" />
      <Label layoutX="250.0" layoutY="50.0" text="Product">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <Button layoutX="300.0" layoutY="350.0" mnemonicParsing="false" onAction="#MainWin" text="Back" />
      <HBox layoutX="200.0" layoutY="150.0" spacing="10">
         <children>
            <Button mnemonicParsing="false" onAction="#ProductAddWindow" style="-fx-font-size: 25px; -fx-background-color: #0074D9; -fx-text-fill: white;" text="Add a Product" />
            <Button mnemonicParsing="false" onAction="#ProductDeleteWindow" style="-fx-font-size: 25px; -fx-background-color: #0074D9; -fx-text-fill: white;" text="Delete a Product" />
            <Button mnemonicParsing="false" onAction="#ProductEditWindow" style="-fx-font-size: 25px; -fx-background-color: #0074D9; -fx-text-fill: white;" text="Edit a Product" />
         </children>
      </HBox>
           
   </children>
</AnchorPane>
